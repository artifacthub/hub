  "crdsExamples": [
    {
      "kind": "HelmRelease",
      "spec": {
        "chart": {
          "spec": {
            "chart": "podinfo",
            "version": "4.0.x",
            "interval": "1m",
            "sourceRef": { "kind": "HelmRepository", "name": "podinfo", "namespace": "flux-system" }
          }
        },
        "values": { "replicaCount": 2 },
        "interval": "5m"
      },
      "metadata": { "name": "podinfo", "namespace": "default" },
      "apiVersion": "helm.toolkit.fluxcd.io/v2beta1"
    },
    {
      "kind": "GitRepository",
      "spec": { "url": "https://github.com/stefanprodan/podinfo", "interval": "1m" },
      "metadata": { "name": "podinfo", "namespace": "default" },
      "apiVersion": "source.toolkit.fluxcd.io/v1beta1"
    },
    {
      "kind": "HelmRepository",
      "spec": { "url": "https://kubernetes-charts.storage.googleapis.com/", "interval": "10m" },
      "metadata": { "name": "stable", "namespace": "default" },
      "apiVersion": "source.toolkit.fluxcd.io/v1beta1"
    },
    {
      "kind": "HelmChart",
      "spec": {
        "chart": "redis",
        "version": "10.5.7",
        "interval": "5m",
        "sourceRef": { "kind": "HelmRepository", "name": "stable" }
      },
      "metadata": { "name": "redis", "namespace": "default" },
      "apiVersion": "source.toolkit.fluxcd.io/v1beta1"
    },
    {
      "kind": "Bucket",
      "spec": {
        "endpoint": "minio.minio.svc.cluster.local:9000",
        "insecure": true,
        "interval": "1m",
        "provider": "generic",
        "secretRef": { "name": "minio-credentials" },
        "bucketName": "podinfo"
      },
      "metadata": { "name": "podinfo", "namespace": "default" },
      "apiVersion": "source.toolkit.fluxcd.io/v1beta1"
    },
    {
      "kind": "Kustomization",
      "spec": {
        "path": "./profiles/default/",
        "timeout": "2m",
        "interval": "5m",
        "sourceRef": { "kind": "GitRepository", "name": "istio" },
        "healthChecks": [{ "kind": "Deployment", "name": "istiod", "namespace": "istio-system" }]
      },
      "metadata": { "name": "istio", "namespace": "istio-system" },
      "apiVersion": "kustomize.toolkit.fluxcd.io/v1beta1"
    },
    {
      "kind": "Provider",
      "spec": {
        "type": "github",
        "address": "https://github.com/stefanprodan/podinfo",
        "secretRef": { "name": "api-token" }
      },
      "metadata": { "name": "podinfo", "namespace": "default" },
      "apiVersion": "notification.toolkit.fluxcd.io/v1beta1"
    },
    {
      "kind": "Alert",
      "spec": {
        "providerRef": { "name": "on-call-slack" },
        "eventSources": [
          { "kind": "GitRepository", "name": "webapp" },
          { "kind": "Bucket", "name": "secrets" },
          { "kind": "Kustomization", "name": "webapp-backend" },
          { "kind": "Kustomization", "name": "webapp-frontend" }
        ],
        "eventSeverity": "info"
      },
      "metadata": { "name": "webapp", "namespace": "default" },
      "apiVersion": "notification.toolkit.fluxcd.io/v1beta1"
    },
    {
      "kind": "Receiver",
      "spec": {
        "type": "github",
        "events": ["ping", "push"],
        "resources": [
          { "kind": "GitRepository", "name": "webapp" },
          { "kind": "HelmRepository", "name": "webapp" }
        ],
        "secretRef": { "name": "webhook-token" }
      },
      "metadata": { "name": "github-receiver", "namespace": "default" },
      "apiVersion": "notification.toolkit.fluxcd.io/v1beta1"
    }
  ],
  "capabilities": "seamless upgrades",
