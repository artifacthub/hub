    "@types/crypto-js": "^4.2.2",
    "@types/jest": "^30.0.0",
    "@types/json-schema": "^7.0.14",
    "@types/json-schema-merge-allof": "^0.6.5",
    "@types/lodash": "^4.17.20",
    "@types/node": "^24.9.2",
    "@types/react": "^19.1.13",
    "@types/react-color": "^3.0.13",
    "@types/react-dom": "^19.1.9",
    "@types/react-syntax-highlighter": "^15.5.13",
    "@types/semver": "^7.7.1",
    "eslint": "^9.39.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "globals": "^16.4.0",
    "jest-fetch-mock": "^3.0.3",
    "jest-mock": "^30.0.5",
    "prettier": "^3.6.2",
    "react-scripts": "^5.0.1",
    "sass": "^1.93.3",
    "shx": "^0.4.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.2"
  },
  "resolutions": {
    "@types/react-syntax-highlighter/**/@types/react": "19.1.13",
    "react-scripts/**/webpack": "5.101.3",
    "react-scripts/**/webpack-dev-server": "5.2.2"
  },
  "proxy": "http://localhost:8000",
  "scripts": {
    "copy:static": "shx rm -rf src/static && shx mkdir src/static && shx cp -r public/static/* src/static",
    "start": "yarn copy:static && NODE_OPTIONS=\"--require ./scripts/wds5-compat.cjs --require ./scripts/webpack-source-map-fix.cjs\" DANGEROUSLY_DISABLE_HOST_CHECK=true DISABLE_ESLINT_PLUGIN=true react-scripts start",
    "build": "yarn copy:static && NODE_OPTIONS=\"--require ./scripts/webpack-source-map-fix.cjs\" INLINE_RUNTIME_CHUNK=false IMAGE_INLINE_SIZE_LIMIT=0 DISABLE_ESLINT_PLUGIN=true react-scripts build",
    "test": "sed -i -e 's/const FORCE_EXIT_DELAY = 500;/const FORCE_EXIT_DELAY = 1000;/g' ./node_modules/jest-worker/build/base/BaseWorkerPool.js && TZ=UTC react-scripts test # See https://github.com/facebook/jest/issues/11354",
    "test:coverage": "TZ=UTC react-scripts test --coverage --watchAll=false",
    "eject": "react-scripts eject",
    "lint": "eslint src --max-warnings 0",
    "lint:fix": "eslint src --max-warnings 0 --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "format:diff": "prettier --list-different \"src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "prettier:fix": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "isready": "yarn format && yarn lint && yarn test --watchAll=false --passWithNoTests --verbose && yarn build"
  },
  "browser": {
    "http": false,
    "https": false
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "transformIgnorePatterns": [
      "node_modules/(?!remark-parse)/"
    ],
    "moduleNameMapper": {
      "unist-util-visit-parents/do-not-use-color": "<rootDir>/node_modules/unist-util-visit-parents/lib/color.js",
      "#minpath": "<rootDir>/node_modules/vfile/lib/minpath.js",
      "#minproc": "<rootDir>/node_modules/vfile/lib/minproc.js",
      "#minurl": "<rootDir>/node_modules/vfile/lib/minurl.js"
    }
  }
}
